{
  "name": "stp",
  "vendor": "vtex",
  "version": "0.0.1",
  "description": "STP chat",
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x"
  },
  "credentialType": "absolute",
  "settingsSchema": {
    "title": "STP",
    "type": "object",
    "properties": {
	  "stpFloatLayout": {
        "title": "Float layout",
        "description": "Do you want the layout to float?",
        "type": "boolean",
		"default": false
      },
      "stpMail": {
        "title": "Email",
        "description": "STP email address",
        "type": "string",
		"default": "chat@stp.es"
      },
	  "stpService": {
        "title": "Service",
        "description": "STP service number",
        "type": "string",
		"default": "14928"
      },	
	
    }
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
